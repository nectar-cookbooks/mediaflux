export MFLUX_HOME="<%= @mflux_home %>"
export MFLUX_SYSTEM_USER=<%= @mflux_user %>
export MFLUX_HOST=`hostname`
<% if @https_port != "" %>
export MFLUX_PORT=<%= @https_port %>
export MFLUX_TRANSPORT=HTTPS
<% else %>
export MFLUX_PORT=<%= @http_port %>
export MFLUX_TRANSPORT=HTTP
<% end %>
export MFLUX_JAVA="<%= @java %>"
