export MFLUX_HOME=<%= @mflux_home %>
export MFLUX_SYSTEM_USER=<%= @mflux_user %>
export MFLUX_HOST=`hostname`
<% if @https_port != "" %>
export MFLUX_PORT=<%= @https_port %>
export MFLUX_TRANSPORT=HTTPS
<% else %>
export MFLUX_PORT=<%= @http_port %>
export MFLUX_TRANSPORT=HTTP
<% end %>
export MFLUX_DOMAIN=system
export MFLUX_USER=manager
export MFLUX_PASSWORD_ENC=<%= @admin_password %>
export MFLUX_RUN_AS_ROOT=<%= if @run_as_root "1" else "0" %>
export MFLUX_UMASK=0007
# For sink file system access
umask 0007